{
  "name": "smart-home-frontend",
  "version": "1.0.0",
  "description": "Smart Home Manager - React Native Expo App",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "start:force": "expo start",
    "start:mobile": "node scripts/mobile-start.js",
    "setup-mobile": "node scripts/get-ip.js && cp .env.mobile .env",
    "setup-localhost": "cp .env.example .env",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:all": "eas build --platform all",
    "submit:android": "eas submit --platform android",
    "submit:ios": "eas submit --platform ios",
    "update": "eas update",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "rmdir /s /q node_modules & rmdir /s /q .expo",
    "setup": "node scripts/setup.js",
    "build-apk": "node build-apk.js"
  },
  "dependencies": {
    "@apollo/client": "^3.8.8",
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "^8.6.0",
    "@react-native-community/netinfo": "11.4.1",
    "@react-native-voice/voice": "^3.2.4",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "ajv": "^8.17.1",
    "clsx": "^2.1.1",
    "expo": "~54.0.0",
    "expo-av": "~16.0.8",
    "expo-barcode-scanner": "~13.0.1",
    "expo-blur": "^15.0.8",
    "expo-camera": "~17.0.10",
    "expo-constants": "~18.0.13",
    "expo-device": "~8.0.10",
    "expo-font": "~14.0.10",
    "expo-haptics": "~15.0.8",
    "expo-image-picker": "~17.0.10",
    "expo-linear-gradient": "~15.0.8",
    "expo-linking": "~8.0.11",
    "expo-notifications": "~0.32.16",
    "expo-router": "~6.0.21",
    "expo-secure-store": "~15.0.8",
    "expo-speech": "~14.0.8",
    "expo-speech-recognition": "^3.0.1",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-system-ui": "~6.0.9",
    "expo-web-browser": "~15.0.10",
    "graphql": "^16.8.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "^3.10.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "react-native-worklets-core": "^1.6.2",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~19.1.10",
    "@types/react-native": "^0.72.8",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-config-expo": "~10.0.0",
    "jest": "^29.2.1",
    "prettier": "^3.1.1",
    "typescript": "~5.9.2"
  },
  "keywords": [
    "smart-home",
    "inventory",
    "management",
    "react-native",
    "expo",
    "mobile"
  ],
  "author": "Smart Home Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "private": true
}
